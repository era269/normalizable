{
  "name": "era269/normalizable",
  "description": "OOP Normalization",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "era269",
      "email": "era269@ukr.net"
    }
  ],
  "require": {
    "php": "^8.0|^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12.88"
  },
  "config": {
    "platform": {
      "php": "8.0.1"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Era269\\Normalizable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Era269\\Normalizable\\Tests\\": "tests"
    }
  }
}
